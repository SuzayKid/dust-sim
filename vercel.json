{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "ALLOW-FROM https://www.canva.com"
        },
        {
          "key": "Content-Security-Policy",
          "value": "frame-ancestors 'self' https://canva.com https://*.canva.com;"
        }
      ]
    }
  ]
}
